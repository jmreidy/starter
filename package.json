{
  "name": "starter",
  "version": "0.1.0",
  "description": "A Starter for kicking off React projects. Sort of like a sourdough starter, but for code.",
  "scripts": {
    "test": "karma start",
    "watch:test": "karma start --auto-watch --no-single-run",
    "validate": "npm-run-all --parallel validate-webpack:* lint test",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "clean-dist": "rimraf dist",
    "copy-files": "echo 'no files to copy'",
    "clean-and-copy": "npm run clean-dist && npm run copy-files",
    "prestart": "npm run clean-dist",
    "start": "webpack-dashboard -- webpack-dev-server --env.dev",
    "prebuild": "npm run clean-dist",
    "prebuild:prod": "npm run clean-and-copy",
    "build": "webpack --env.dev",
    "build:prod": "webpack --env.prod",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "author": "Justin Reidy <jmreidy@rzrsharp.net>",
  "license": "ISC",
  "dependencies": {
    "milligram": "^1.1.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-observable": "^0.9.0",
    "rxjs": "^5.0.0-beta.11"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "karma": "^1.1.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-webpack": "^1.8.0",
    "milligram": "^1.1.0",
    "mocha": "^3.0.2",
    "npm-run-all": "^2.3.0",
    "postcss-import": "^8.1.0",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "^3.0.0-beta.1",
    "rimraf": "^2.5.4",
    "rucksack-css": "^0.8.6",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.20",
    "webpack-dashboard": "^0.1.1",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-validator": "^2.2.7"
  }
}
